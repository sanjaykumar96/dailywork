package com.cts.march1;

public class PrintIfElse {
	 public static void main(String args[])
	    {
	        
	        for(int i=0;i<2;i++)
	        {
	        if(i==1)
	        {
	            System.out.println("Java");
	              
	        }   
	        else

	            System.out.println("I Like");

	        }       
	    }

}
