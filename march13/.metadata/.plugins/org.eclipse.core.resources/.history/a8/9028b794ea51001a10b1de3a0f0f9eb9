package com.cognizant;

public class Print {
public static void main(String[] args) {
	GetterSetter e=new GetterSetter();
	System.out.println("Before salary = " + e.getAge());
	System.out.println("Before Designation = " + e.getSalary());
	
	e.setAge(25);
	e.setSalary(25000);
	System.out.println("Before salary = " + e.getAge());
	System.out.println("Before Designation = " + e.getSalary());
}
}
