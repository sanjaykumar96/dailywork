<script type="text/javascript">
	function Employee(firstName) {
		this.firstName = firstName;

	}
	
	var e1=new Employee("Kartik");
	var e2=new Employee("Kavya");
	e1.getEmployeeName=function(){
		return this.firstName;
	}
	e2.getEmployeeName=function(){
		return this.firstName;
	}
	
	document.write("e1 name= "+e1.getEmployeeName());<br>
	document.write("e2 name= "+e2.getEmployeeName());
	
</script>
