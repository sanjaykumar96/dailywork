package com.cts.march1;

public class PrintIfElse {
	 public static void main(String args[])
	    {
	        boolean one=true;
	        for(int i=0;i<2;i++)
	        {
	        if(i==1)
	        {
	            System.out.print("Java");
	            one=false;  
	        }   
	        else

	            System.out.println("I Like");

	        }       
	    }

}
